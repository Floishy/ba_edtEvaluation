    <a:note rdf:datatype="&xsd;string">Dissertation</a:note>
    <a:title rdf:datatype="&xsd;string">Kennzahlenbasiertes Management von Geschftsprozessen mit Petri-Netzen</a:title>
    <a:year rdf:datatype="&xsd;string">2006</a:year>
    <a:author rdf:resource="&e;id2058instance"/>
    <a:isAbout rdf:resource="&b;id124instance"/>
</a:Book>

<a:InProceedings rdf:about="&h;id1279instance">
